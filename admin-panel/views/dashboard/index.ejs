<!-- هدر داشبورد با افکت‌های مدرن -->
<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-4 border-bottom">
  <h1 class="h2 d-flex align-items-center animated-element">
    <i class="fas fa-tachometer-alt me-2 text-primary"></i>
    داشبورد مدیریت
    <span class="badge bg-primary ms-2 fs-6">نسخه 2.0</span>
  </h1>
  <div class="btn-toolbar mb-2 mb-md-0">
    <div class="btn-group me-2">
      <button type="button" class="btn btn-sm btn-outline-secondary" id="exportDashboardReport">
        <i class="fas fa-file-export me-1"></i>
        گزارش
      </button>
      <button type="button" class="btn btn-sm btn-outline-secondary" id="refreshDashboard">
        <i class="fas fa-sync-alt me-1"></i>
        بروزرسانی
      </button>
      <button type="button" class="btn btn-sm btn-outline-primary" id="customizeDashboard">
        <i class="fas fa-cog me-1"></i>
        شخصی‌سازی
      </button>
    </div>
    <div class="dropdown">
      <button type="button" class="btn btn-sm btn-outline-secondary dropdown-toggle" data-bs-toggle="dropdown">
        <i class="fas fa-calendar me-1"></i>
        <span id="date-range-text">امروز</span>
      </button>
      <ul class="dropdown-menu">
        <li><a class="dropdown-item" href="#" data-range="today">امروز</a></li>
        <li><a class="dropdown-item" href="#" data-range="yesterday">دیروز</a></li>
        <li><a class="dropdown-item" href="#" data-range="week">هفته جاری</a></li>
        <li><a class="dropdown-item" href="#" data-range="month">ماه جاری</a></li>
        <li><hr class="dropdown-divider"></li>
        <li><a class="dropdown-item" href="#" data-range="custom">بازه سفارشی...</a></li>
      </ul>
    </div>
  </div>
</div>

<!-- کانتینر توست‌ها -->
<div id="toast-container" class="toast-container position-fixed top-0 end-0 p-3"></div>

<!-- نوار وضعیت سیستم -->
<div class="system-status-bar bg-light p-2 rounded-3 mb-4 animated-element">
  <div class="row g-0 align-items-center">
    <div class="col-auto pe-3">
      <span class="text-success d-flex align-items-center">
        <i class="fas fa-circle fs-6 me-1"></i>
        <span class="text-dark fw-500">وضعیت سیستم:</span>
      </span>
    </div>
    <div class="col-auto px-2 border-start border-end">
      <small class="text-muted">ربات: <span class="text-success">آنلاین</span></small>
    </div>
    <div class="col-auto px-2 border-end">
      <small class="text-muted">دیتابیس: <span class="text-success">متصل</span></small>
    </div>
    <div class="col-auto px-2 border-end">
      <small class="text-muted">کاربران آنلاین: <span class="fw-bold"><%= stats.onlineUsers || 0 %></span></small>
    </div>
    <div class="col-auto px-2 border-end">
      <small class="text-muted">آپتایم: <span class="fw-bold"><%= stats.uptime || '0 ساعت' %></span></small>
    </div>
    <div class="col-auto px-2">
      <small class="text-muted">آخرین بروزرسانی: <span class="fw-bold"><%= helpers.formatDate(new Date()) %></span></small>
    </div>
    <div class="col text-end">
      <button class="btn btn-sm btn-link text-decoration-none" id="restore-widgets-btn" style="display: none;">
        <i class="fas fa-undo-alt me-1"></i>
        بازگرداندن ویجت‌ها
      </button>
    </div>
  </div>
</div>

<!-- آمار کلی -->
<div id="draggable-widgets" class="row g-3 mb-4">
  <!-- کاربران -->
  <div class="col-md-3 animated-element" id="widget-users">
    <div class="dashboard-widget stat-card">
      <div class="widget-header bg-primary d-flex justify-content-between p-2 text-white">
        <div>
          <i class="fas fa-users"></i>
          <span class="ms-1">کاربران</span>
        </div>
        <div class="widget-controls">
          <i class="fas fa-arrows-alt widget-handle" style="cursor: move;"></i>
          <i class="fas fa-sync-alt widget-control" data-action="refresh"></i>
          <i class="fas fa-minus widget-control" data-action="collapse"></i>
        </div>
      </div>
      <div class="widget-body card-body">
        <div class="d-flex align-items-center">
          <div class="icon-container bg-primary me-3">
            <i class="fas fa-users fa-2x text-white"></i>
          </div>
          <div>
            <div class="d-flex align-items-center">
              <h3 class="mb-0" id="totalUsers" data-value="<%= stats.totalUsers %>"><%= stats.totalUsers %></h3>
              <span id="userChangeIndicator" class="ms-2 text-<%= stats.newUsers24h > 0 ? 'success' : 'secondary' %>">
                <i class="fas fa-<%= stats.newUsers24h > 0 ? 'arrow-up' : 'minus' %>"></i>
                <%= stats.newUsers24h %>
              </span>
            </div>
            <div class="text-muted mt-2">
              <small>
                <i class="fas fa-user-plus text-success me-1"></i>
                <%= stats.newUsers24h %> کاربر جدید امروز
              </small>
            </div>
            <div class="progress mt-2" style="height: 6px;">
              <div class="progress-bar" role="progressbar" style="width: <%= (stats.activeUsers / stats.totalUsers) * 100 %>%"></div>
            </div>
            <div class="d-flex justify-content-between mt-1">
              <small class="text-muted"><%= stats.activeUsers %> کاربر فعال</small>
              <small class="text-muted"><%= Math.round((stats.activeUsers / stats.totalUsers) * 100) %>% نرخ فعالیت</small>
            </div>
          </div>
        </div>
        <div class="mt-3 text-end">
          <a href="/users" class="btn btn-sm btn-outline-primary">
            <i class="fas fa-users me-1"></i>
            مدیریت کاربران
          </a>
        </div>
      </div>
    </div>
  </div>
  
  <!-- کلن‌ها -->
  <div class="col-md-3 animated-element" id="widget-clans">
    <div class="dashboard-widget stat-card">
      <div class="widget-header bg-success d-flex justify-content-between p-2 text-white">
        <div>
          <i class="fas fa-flag"></i>
          <span class="ms-1">کلن‌ها</span>
        </div>
        <div class="widget-controls">
          <i class="fas fa-arrows-alt widget-handle" style="cursor: move;"></i>
          <i class="fas fa-sync-alt widget-control" data-action="refresh"></i>
          <i class="fas fa-minus widget-control" data-action="collapse"></i>
        </div>
      </div>
      <div class="widget-body card-body">
        <div class="d-flex align-items-center">
          <div class="icon-container bg-success me-3">
            <i class="fas fa-flag fa-2x text-white"></i>
          </div>
          <div>
            <div class="d-flex align-items-center">
              <h3 class="mb-0" id="totalClans"><%= stats.totalClans %></h3>
              <span class="ms-2 text-<%= stats.newClans24h > 0 ? 'success' : 'secondary' %>">
                <i class="fas fa-<%= stats.newClans24h > 0 ? 'arrow-up' : 'minus' %>"></i>
                <%= stats.newClans24h || 0 %>
              </span>
            </div>
            <div class="text-muted mt-2">
              <small>
                <i class="fas fa-flag text-success me-1"></i>
                <%= stats.newClans24h || 0 %> کلن جدید امروز
              </small>
            </div>
            <div class="progress mt-2" style="height: 6px;">
              <div class="progress-bar bg-success" role="progressbar" style="width: <%= (stats.totalClans / (stats.totalClans + 10)) * 100 %>%"></div>
            </div>
            <div class="d-flex justify-content-between mt-1">
              <small class="text-muted">میانگین اعضا: <%= stats.avgClanMembers || 0 %></small>
              <small class="text-muted"><%= stats.activeClanWars || 0 %> جنگ فعال</small>
            </div>
          </div>
        </div>
        <div class="mt-3 text-end">
          <a href="/clans" class="btn btn-sm btn-outline-success">
            <i class="fas fa-flag me-1"></i>
            مدیریت کلن‌ها
          </a>
        </div>
      </div>
    </div>
  </div>
  
  <!-- Ccoin -->
  <div class="col-md-3 animated-element" id="widget-economy">
    <div class="dashboard-widget stat-card">
      <div class="widget-header bg-warning d-flex justify-content-between p-2 text-dark">
        <div>
          <i class="fas fa-coins"></i>
          <span class="ms-1">اقتصاد</span>
        </div>
        <div class="widget-controls">
          <i class="fas fa-arrows-alt widget-handle" style="cursor: move;"></i>
          <i class="fas fa-sync-alt widget-control" data-action="refresh"></i>
          <i class="fas fa-minus widget-control" data-action="collapse"></i>
        </div>
      </div>
      <div class="widget-body card-body">
        <div class="d-flex align-items-center">
          <div class="icon-container bg-warning me-3">
            <i class="fas fa-coins fa-2x text-white"></i>
          </div>
          <div>
            <div class="d-flex align-items-center">
              <h3 class="mb-0" id="totalCcoin"><%= helpers.formatNumber(stats.totalCcoin) %></h3>
              <span class="ms-2 text-<%= stats.economyChange > 0 ? 'success' : 'danger' %>">
                <i class="fas fa-<%= stats.economyChange > 0 ? 'arrow-up' : 'arrow-down' %>"></i>
                <%= helpers.formatNumber(Math.abs(stats.economyChange)) %>
              </span>
            </div>
            <div class="text-muted mt-2">
              <small>
                <i class="fas fa-exchange-alt me-1"></i>
                <%= helpers.formatNumber(stats.transactions24h || 0) %> تراکنش در 24 ساعت
              </small>
            </div>
            <div class="progress mt-2" style="height: 6px;">
              <div class="progress-bar bg-warning" role="progressbar" style="width: <%= (stats.totalWallet / stats.totalCcoin) * 100 %>%"></div>
            </div>
            <div class="d-flex justify-content-between mt-1">
              <small class="text-muted">کیف پول: <%= helpers.formatNumber(stats.totalWallet || 0) %></small>
              <small class="text-muted">بانک: <%= helpers.formatNumber(stats.totalBank || 0) %></small>
            </div>
          </div>
        </div>
        <div class="mt-3 text-end">
          <a href="/economy" class="btn btn-sm btn-outline-warning">
            <i class="fas fa-chart-line me-1"></i>
            مدیریت اقتصاد
          </a>
        </div>
      </div>
    </div>
  </div>
  
  <!-- ماموریت‌ها -->
  <div class="col-md-3 animated-element" id="widget-quests">
    <div class="dashboard-widget stat-card">
      <div class="widget-header bg-info d-flex justify-content-between p-2 text-white">
        <div>
          <i class="fas fa-tasks"></i>
          <span class="ms-1">ماموریت‌ها</span>
        </div>
        <div class="widget-controls">
          <i class="fas fa-arrows-alt widget-handle" style="cursor: move;"></i>
          <i class="fas fa-sync-alt widget-control" data-action="refresh"></i>
          <i class="fas fa-minus widget-control" data-action="collapse"></i>
        </div>
      </div>
      <div class="widget-body card-body">
        <div class="d-flex align-items-center">
          <div class="icon-container bg-info me-3">
            <i class="fas fa-tasks fa-2x text-white"></i>
          </div>
          <div>
            <div class="d-flex align-items-center">
              <h3 class="mb-0" id="activeQuests"><%= stats.activeQuests %></h3>
              <span class="ms-2 text-info">
                <i class="fas fa-list"></i>
                فعال
              </span>
            </div>
            <div class="text-muted mt-2">
              <small>
                <i class="fas fa-check text-success me-1"></i>
                <%= stats.completedQuests7d || 0 %> ماموریت تکمیل شده در هفته
              </small>
            </div>
            <div class="progress mt-2" style="height: 6px;">
              <div class="progress-bar bg-info" role="progressbar" style="width: <%= (stats.completedQuests7d / (stats.completedQuests7d + stats.activeQuests)) * 100 %>%"></div>
            </div>
            <div class="d-flex justify-content-between mt-1">
              <small class="text-muted">نرخ تکمیل: <%= Math.round((stats.completedQuests7d / (stats.completedQuests7d + stats.activeQuests)) * 100) %>%</small>
              <small class="text-muted">کل: <%= stats.activeQuests + stats.completedQuests7d %></small>
            </div>
          </div>
        </div>
        <div class="mt-3 text-end">
          <a href="/quests" class="btn btn-sm btn-outline-info">
            <i class="fas fa-tasks me-1"></i>
            مدیریت ماموریت‌ها
          </a>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row g-3 mb-4">
  <!-- نمودار کاربران فعال -->
  <div class="col-md-8 animated-element">
    <div class="card border-0 shadow-sm h-100">
      <div class="card-header bg-white d-flex justify-content-between align-items-center">
        <h5 class="card-title mb-0 d-flex align-items-center">
          <i class="fas fa-chart-line me-2 text-primary"></i>
          کاربران فعال (7 روز اخیر)
          <i class="fas fa-question-circle help-tooltip ms-2" data-tooltip="این نمودار تعداد کاربران فعال را در 7 روز گذشته نشان می‌دهد"></i>
        </h5>
        <div class="btn-group">
          <button type="button" class="btn btn-sm btn-outline-secondary active" data-chart-period="week">7 روز</button>
          <button type="button" class="btn btn-sm btn-outline-secondary" data-chart-period="month">30 روز</button>
          <button type="button" class="btn btn-sm btn-outline-secondary" data-chart-period="custom">سفارشی</button>
        </div>
      </div>
      <div class="card-body">
        <div class="chart-container">
          <canvas id="activeUsersChart"></canvas>
        </div>
      </div>
      <div class="card-footer bg-white border-0 d-flex justify-content-between">
        <div class="d-flex">
          <div class="me-3 text-center">
            <div class="small text-muted">میانگین روزانه</div>
            <div class="fs-5 fw-bold text-success">
              <%= Math.round(activeUsersHistory.reduce((sum, item) => sum + item.count, 0) / activeUsersHistory.length) %>
            </div>
          </div>
          <div class="me-3 text-center">
            <div class="small text-muted">بیشترین</div>
            <div class="fs-5 fw-bold text-primary">
              <%= Math.max(...activeUsersHistory.map(item => item.count)) %>
            </div>
          </div>
          <div class="text-center">
            <div class="small text-muted">کمترین</div>
            <div class="fs-5 fw-bold text-secondary">
              <%= Math.min(...activeUsersHistory.map(item => item.count)) %>
            </div>
          </div>
        </div>
        <div>
          <button class="btn btn-sm btn-outline-primary" data-bs-toggle="tooltip" title="دانلود داده‌های نمودار">
            <i class="fas fa-download"></i>
          </button>
        </div>
      </div>
    </div>
  </div>
  
  <!-- نمودار بازی‌ها و آمار سریع -->
  <div class="col-md-4 animated-element">
    <div class="card border-0 shadow-sm h-100">
      <div class="card-header bg-white d-flex justify-content-between align-items-center">
        <h5 class="card-title mb-0 d-flex align-items-center">
          <i class="fas fa-gamepad me-2 text-success"></i>
          آمار بازی‌ها
          <i class="fas fa-question-circle help-tooltip ms-2" data-tooltip="این نمودار توزیع بازی‌های انجام شده را نشان می‌دهد"></i>
        </h5>
        <div class="dropdown">
          <button class="btn btn-sm btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown">
            امروز
          </button>
          <ul class="dropdown-menu">
            <li><a class="dropdown-item" href="#">امروز</a></li>
            <li><a class="dropdown-item" href="#">هفته جاری</a></li>
            <li><a class="dropdown-item" href="#">ماه جاری</a></li>
          </ul>
        </div>
      </div>
      <div class="card-body">
        <div class="chart-container">
          <canvas id="gamesChart"></canvas>
        </div>
      </div>
      <div class="card-footer bg-white border-0">
        <div class="row g-2 text-center">
          <div class="col-4">
            <div class="bg-light rounded p-2">
              <div class="small text-muted">تعداد کل</div>
              <div class="fs-5 fw-bold">
                <%= gameStats.coinflip + gameStats.rps + gameStats.numberguess + gameStats.dice + gameStats.other %>
              </div>
            </div>
          </div>
          <div class="col-4">
            <div class="bg-light rounded p-2">
              <div class="small text-muted">محبوب‌ترین</div>
              <div class="fs-5 fw-bold text-primary">
                <% 
                  let mostPopular = "سکه";
                  let max = gameStats.coinflip;
                  if (gameStats.rps > max) { mostPopular = "سنگ کاغذ قیچی"; max = gameStats.rps; }
                  if (gameStats.numberguess > max) { mostPopular = "حدس عدد"; max = gameStats.numberguess; }
                  if (gameStats.dice > max) { mostPopular = "تاس"; max = gameStats.dice; }
                %>
                <%= mostPopular %>
              </div>
            </div>
          </div>
          <div class="col-4">
            <div class="bg-light rounded p-2">
              <div class="small text-muted">برد/باخت</div>
              <div class="fs-5 fw-bold text-<%= stats.winRate > 50 ? 'success' : 'danger' %>">
                <%= stats.winRate || 50 %>%
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row g-3">
  <!-- کاربران فعال اخیر -->
  <div class="col-md-6 animated-element">
    <div class="card border-0 shadow-sm h-100">
      <div class="card-header bg-white d-flex justify-content-between align-items-center">
        <h5 class="card-title mb-0 d-flex align-items-center">
          <i class="fas fa-user-clock me-2 text-primary"></i>
          کاربران فعال اخیر
        </h5>
        <a href="/users" class="btn btn-sm btn-primary">
          <i class="fas fa-external-link-alt me-1"></i>
          مشاهده همه
        </a>
      </div>
      <div class="card-body p-0">
        <div class="table-responsive">
          <table class="table table-hover mb-0">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">نام کاربری</th>
                <th scope="col">آخرین فعالیت</th>
                <th scope="col">موجودی</th>
              </tr>
            </thead>
            <tbody id="recentUsersList">
              <% if (recentUsers && recentUsers.length > 0) { %>
                <% recentUsers.forEach((user, index) => { %>
                  <tr>
                    <th scope="row"><%= index + 1 %></th>
                    <td>
                      <a href="/users/<%= user.id %>" class="text-decoration-none d-flex align-items-center">
                        <div class="user-avatar rounded-circle bg-light me-2 d-flex align-items-center justify-content-center" style="width: 30px; height: 30px;">
                          <i class="fas fa-user text-primary"></i>
                        </div>
                        <%= user.username %>
                        <% if (user.isBanned) { %>
                          <span class="badge bg-danger ms-1">مسدود</span>
                        <% } %>
                        <% if (user.isPremium) { %>
                          <span class="badge bg-warning text-dark ms-1">
                            <i class="fas fa-crown"></i>
                          </span>
                        <% } %>
                      </a>
                    </td>
                    <td><span data-time="<%= user.lastSeen %>"><%= helpers.timeAgo(user.lastSeen) %></span></td>
                    <td><%= helpers.formatNumber(user.wallet + user.bank) %> <i class="fas fa-coins text-warning"></i></td>
                  </tr>
                <% }) %>
              <% } else { %>
                <tr>
                  <td colspan="4" class="text-center py-4">
                    <i class="fas fa-user-slash fa-2x mb-2 text-muted"></i>
                    <p class="text-muted mb-0">هیچ کاربر فعالی یافت نشد.</p>
                  </td>
                </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
      <div class="card-footer bg-white">
        <div class="d-flex justify-content-between align-items-center">
          <small class="text-muted">یافت شده: <%= recentUsers ? recentUsers.length : 0 %> کاربر</small>
          <div class="btn-group">
            <button type="button" class="btn btn-sm btn-outline-secondary">
              <i class="fas fa-sync-alt me-1"></i>
              بروزرسانی
            </button>
            <button type="button" class="btn btn-sm btn-outline-secondary">
              <i class="fas fa-filter me-1"></i>
              فیلتر
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
  
  <!-- رویدادهای اخیر -->
  <div class="col-md-6 animated-element">
    <div class="card border-0 shadow-sm h-100">
      <div class="card-header bg-white d-flex justify-content-between align-items-center">
        <h5 class="card-title mb-0 d-flex align-items-center">
          <i class="fas fa-history me-2 text-primary"></i>
          رویدادهای اخیر
        </h5>
        <a href="/logs" class="btn btn-sm btn-primary">
          <i class="fas fa-external-link-alt me-1"></i>
          مشاهده همه
        </a>
      </div>
      <div class="card-body p-0">
        <div class="table-responsive">
          <table class="table table-hover mb-0">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">نوع</th>
                <th scope="col">پیام</th>
                <th scope="col">زمان</th>
              </tr>
            </thead>
            <tbody id="recentEventsList">
              <% if (recentEvents && recentEvents.length > 0) { %>
                <% recentEvents.forEach((event, index) => { %>
                  <tr>
                    <th scope="row"><%= index + 1 %></th>
                    <td>
                      <% 
                        let badgeClass = 'bg-secondary';
                        let iconClass = 'fas fa-info-circle';
                        
                        if (event.type === 'user') {
                          badgeClass = 'bg-primary';
                          iconClass = 'fas fa-user';
                        } else if (event.type === 'transaction') {
                          badgeClass = 'bg-success';
                          iconClass = 'fas fa-exchange-alt';
                        } else if (event.type === 'game') {
                          badgeClass = 'bg-warning text-dark';
                          iconClass = 'fas fa-gamepad';
                        } else if (event.type === 'clan') {
                          badgeClass = 'bg-info';
                          iconClass = 'fas fa-flag';
                        } else if (event.type === 'system') {
                          badgeClass = 'bg-dark';
                          iconClass = 'fas fa-cog';
                        }
                      %>
                      <span class="badge <%= badgeClass %> d-flex align-items-center" style="width: fit-content;">
                        <i class="<%= iconClass %> me-1"></i>
                        <%= event.type %>
                      </span>
                    </td>
                    <td><%= event.message %></td>
                    <td><span data-time="<%= event.timestamp %>"><%= helpers.timeAgo(event.timestamp) %></span></td>
                  </tr>
                <% }) %>
              <% } else { %>
                <tr>
                  <td colspan="4" class="text-center py-4">
                    <i class="fas fa-inbox fa-2x mb-2 text-muted"></i>
                    <p class="text-muted mb-0">هیچ رویدادی یافت نشد.</p>
                  </td>
                </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
      <div class="card-footer bg-white">
        <div class="d-flex justify-content-between align-items-center">
          <small class="text-muted">یافت شده: <%= recentEvents ? recentEvents.length : 0 %> رویداد</small>
          <div class="btn-group">
            <button type="button" class="btn btn-sm btn-outline-secondary">
              <i class="fas fa-sync-alt me-1"></i>
              بروزرسانی
            </button>
            <button type="button" class="btn btn-sm btn-outline-secondary">
              <i class="fas fa-filter me-1"></i>
              فیلتر
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

